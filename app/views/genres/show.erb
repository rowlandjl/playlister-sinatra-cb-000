<h1>Genre: <%= @genres.name %></h1>

<h3>Artists</h3>
<ul>
  <% @genres.artists.each do |artist| %>
    <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></a></li>
  <% end %>
</ul>

<h3>Songs</h3>
<ul>
  <% @genres.songs.each do |song| %>
    <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
  <% end %>
</ul>
